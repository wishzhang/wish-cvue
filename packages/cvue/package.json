{
  "name": "@wele/cvue",
  "version": "0.1.1",
  "author": "wishzhang<wishzhang.io@qq.com>",
  "description": "A Vue.js 3 UI library base on Element Plus",
  "keywords": [
    "@wish",
    "cvue",
    "element-plus",
    "element",
    "component library",
    "ui framework",
    "ui",
    "vue"
  ],
  "homepage": "https://wishzhang.github.io/wish-cvue",
  "license": "MIT",
  "main": "dist/index.full.js",
  "module": "dist/index.full.mjs",
  "types": "es/cvue/index.d.ts",
  "unpkg": "dist/index.full.js",
  "jsdelivr": "dist/index.full.js",
  "exports": {
    ".": {
      "types": "./es/cvue/index.d.ts",
      "import": "./es/cvue/index.mjs",
      "require": "./lib/cvue/index.js"
    },
    "./es": "./es/cvue/index.mjs",
    "./lib": "./lib/cvue/index.js",
    "./es/*.mjs": "./es/cvue/*.mjs",
    "./es/*": "./es/cvue/*.mjs",
    "./lib/*.js": "./lib/cvue/*.js",
    "./lib/*": "./lib/cvue/*.js",
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wishzhang/wish-cvue.git"
  },
  "bugs": {
    "url": "https://github.com/wishzhang/wish-cvue/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "style": "dist/index.css",
  "sideEffects": [
    "dist/*",
    "theme-chalk/**/*.css",
    "theme-chalk/src/**/*.scss"
  ],
  "dependencies": {
    "@types/lodash-es": "^4.17.6",
    "lodash-es": "^4.17.21",
    "@vueuse/core": "^9.13.0"
  },
  "peerDependencies": {
    "vue": ">=3.2.45",
    "element-plus": ">=2.2.29"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
